{
    "name": "Arduino Flasher",
    "description": "Programs an arduino with a given hex file.",
    "version": "0.2",
    "manifest_version": 2,
    "app": {
        "background": {
          "scripts": ["./js/background.js"]
        }
    },
    "permissions": [
        "serial",
        "fileSystem",
        "usb",
        {
            "usbDevices": [
                {
                    "vendorId": 4617,
                    "productId": 8706
                }
            ]
        }
    ]
}
